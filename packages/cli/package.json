{
  "name": "@classicuo/cli",
  "version": "0.0.6",
  "description": "CLI tool for shard management",
  "author": "ClassicUO Team",
  "bin": "./bin/run",
  "homepage": "https://github.com/ClassicUO/classicuo-web",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "ClassicUO/classicuo-web",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.13.1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "@terascope/fetch-github-release": "^0.8.7",
    "axios": "^0.27.2",
    "fp-ts": "^2.12.2",
    "fp-ts-contrib": "^0.1.29",
    "vcdiff-wasm": "^1.0.10",
    "zod": "^3.17.10"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.8.1",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4"
  },
  "oclif": {
    "bin": "@classicuo/cli",
    "dirname": "@classicuo/cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": ":",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      },
      "patching": {
        "description": "Create MUL/UOP .patch files"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "rm -rf bin/classicuo-web-diff-tool-* && yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "patch": "./bin/dev patching:create-v1",
    "hello": "./bin/dev hello friend -f hi"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bugs": "https://github.com/ClassicUO/classicuo-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
